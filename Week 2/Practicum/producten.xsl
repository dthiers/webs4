<?xml version="1.0"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"><xsl:template match="producten">  <html>    <head>      <title>De Aanbiedingen van Deze Week</title>    </head>    <body bgcolor="lightblue">      <xsl:apply-templates/>    </body>  </html></xsl:template><xsl:template match="boek">  <p>    <b>Titel:<xsl:value-of select="titel"/></b>    <ul>      <li>Schrijver:<xsl:value-of select="schrijver"/></li>      <li>ISBN:<xsl:value-of select="isbn"/></li>      <li>Omschrijving:<xsl:value-of select="omschrijving"/></li>    </ul>  </p>  <hr></hr></xsl:template><xsl:template match="dvd">  <p>    <b>Titel:<xsl:value-of select="titel"/></b>    <ul>      <li>Artiest:<xsl:value-of select="artiest"/></li>      <li>Speelduur:<xsl:value-of select="speelduur"/></li>      <li>Beschrijving:<xsl:value-of select="beschrijving"/></li>    </ul>  </p>  <hr></hr></xsl:template></xsl:stylesheet>